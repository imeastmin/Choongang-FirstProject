<?xml version="1.0" encoding="UTF-8" ?>

<mapper namespace="housens">

	<!-- 숙소 저장 -->
	<insert id="house_insert">
		insert into house
		(house_num,house_name,house_address,house_x_abs,house_y_abs,
		house_photo,house_capacity,house_form,house_detail,house_share,house_authority)
		values(house_seq.nextval,#{house_name},#{house_address},#{house_x_abs},
		#{house_y_abs},#{house_photo},#{house_capacity},#{house_form},#{house_detail},#{house_share},#{house_authority})
	</insert>


</mapper>






















